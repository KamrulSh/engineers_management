<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="technical_technology_tree" model="ir.ui.view">
        <field name="name">technical.technology.tree</field>
        <field name="model">technical.technology</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="technology_name"/>
            </tree>
        </field>
    </record>

    <record id="technical_technology_form" model="ir.ui.view">
        <field name="name">technical.technology.form</field>
        <field name="model">technical.technology</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="technology_name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>