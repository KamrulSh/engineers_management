<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="engineer_position_tree" model="ir.ui.view">
        <field name="name">engineer.position.tree</field>
        <field name="model">engineer.position</field>
        <field name="arch" type="xml">
            <tree>
                <field name="position_name"/>
            </tree>
        </field>
    </record>

    <record id="engineer_position_form" model="ir.ui.view">
        <field name="name">engineer.position.form</field>
        <field name="model">engineer.position</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="position_name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>